//
// Module
//
with [get-view Outline | get-tree] {
    get-item "mymodule" | get-property "getData().getClassName()" 
        | equals "org.eclipse.dltk.internal.core.SourceType" | verify-true
    get-item "mymodule" | get-property "getData().occurrenceCount" | equals 1 | verify-true
}
with [get-view "Script Explorer" | get-tree] {
    get-item "projectWithModule/src/mymodule.lua/mymodule" | get-property "getData().getClassName()" 
        | equals "org.eclipse.dltk.internal.core.SourceType" | verify-true
    get-item "projectWithModule/src/mymodule.lua/mymodule" | get-property "getData().occurrenceCount" | equals 1 | verify-true
}

