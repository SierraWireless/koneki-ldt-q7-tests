------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

// Open main.lua file
with [get-view "Script Explorer" | get-tree] {
    select "myproject/src/main.lua" | double-click
}

// Open the content-assist after a few letters typed
with [get-editor main | get-text-viewer] {
    set-caret-pos 2 1
    type-text "local str = require (\"string"
    key-type End
    set-caret-pos 2 31
    key-type Enter
    type-text "str."
    key-type "M1+SPACE"
}

get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | select "lower\\(s\\) - string" 
    | click -default
    
get-editor main | get-editbox | get-property text | equals "local function main()\n"
    + "local str = require (\"string\")\n"
    + "str.lower(s)\n"
    + "end\n"
    + "main()" | verify-true
