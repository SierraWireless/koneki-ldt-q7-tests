------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

// Open main.lua file
with [get-view "Script Explorer" | get-tree] {
    select "newProject_execEnvOnCreation/src/main.lua" | double-click
}

// Open the content-assist after a few letters typed
with [get-editor main | get-text-viewer] {
    set-caret-pos 2 1
    type-text "local file = io.open(\"myFile\",\"r\""
    key-type Enter
    key-type Enter
    type-text "file."
    key-type "M1+SPACE"
}

get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | select "close\\(self\\) - file" 
    | click -default
    
get-editor main | get-editbox | get-property text | equals "local function main()\n"
    + "local file = io.open(\"myFile\",\"r\")\n"
    + "file.close(self)\n"
    + "end\n"
    + "main()" | verify-true
