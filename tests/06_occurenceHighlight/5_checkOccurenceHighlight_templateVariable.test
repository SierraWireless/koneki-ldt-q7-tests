/*********************
*	Created by: Simon Gaestel
*	Creation date: 07/17/2012
*********************/

// Open the main.lua file
with [get-view "Script Explorer" | get-tree] {
    select "Project_noExecEnv/src/main.lua" | double-click
}

// Enter a template with pre-filled variables and re-use them in the template
with [get-editor main | get-text-viewer] {
    set-caret-pos 2 1
    type-text "fori"
    key-type "M1+SPACE"
    key-type Enter
    key-type Tab -times 2
    type-text "{}"
    key-type Tab
    type-text "key = 1"
    key-type Enter
    type-text "print(var)"
    set-caret-pos 6 1
    type-text "key = 2"
    key-type Enter
    type-text "print(var)"
    key-type Enter
	set-caret-pos 2 9
    key-type Left
}

// Check the parameters are highlighted only in the template
with [get-editor main | get-editbox] {
    get-property "getStyleRanges().StyleRange[9].start" | equals 26 | verify-true
    get-property "getStyleRanges().StyleRange[9].length" | equals 3 | verify-true
    get-property "getStyleRanges().StyleRange[9].font" | equals null | verify-true
    get-property "getStyleRanges().StyleRange[9].foreground" | equals "#676767" | verify-true
    get-property "getStyleRanges().StyleRange[9].background" | equals "#D4D4D4" | verify-true
    
    get-property "getStyleRanges().StyleRange[21].start" | equals 53 | verify-true
    get-property "getStyleRanges().StyleRange[21].length" | equals 3 | verify-true
