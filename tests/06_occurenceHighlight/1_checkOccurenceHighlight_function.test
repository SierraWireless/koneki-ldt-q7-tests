------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

/*********************
*	Created by: Simon Gaestel
*	Creation date: 07/16/2012
*********************/

// Open the main.lua file
with [get-view "Script Explorer" | get-tree] {
    select "Project_noExecEnv/src/main.lua" | double-click
}

// Click on "main" in the text editor
with [get-editor main | get-text-viewer] {
	set-caret-pos 4 6
    key-type Left
}
wait 2000

// Check all occurences of "main" are highlighted
with [get-editor main | get-editbox] {
    get-property "getStyleRanges().StyleRange[4].start" | equals 15 | verify-true
    get-property "getStyleRanges().StyleRange[4].length" | equals 4 | verify-true
    get-property "getStyleRanges().StyleRange[4].font" | equals null | verify-true
    get-property "getStyleRanges().StyleRange[4].foreground" | equals "#676767" | verify-true
    get-property "getStyleRanges().StyleRange[4].background" | equals "#D4D4D4" | verify-true
    
    get-property "getStyleRanges().StyleRange[9].start" | equals 27 | verify-true
    get-property "getStyleRanges().StyleRange[9].length" | equals 4 | verify-true
    get-property "getStyleRanges().StyleRange[9].font" | equals null | verify-true
    get-property "getStyleRanges().StyleRange[9].foreground" | equals "#676767" | verify-true
    get-property "getStyleRanges().StyleRange[9].background" | equals "#D4D4D4" | verify-true
}

