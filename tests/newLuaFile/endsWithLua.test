Content-Type: text/ecl
Entry-Name: .content

// create lua file
get-menu "File/New/Other..." | click
get-window New | get-tree | select "Lua/Lua File" | double-click
with [get-window "New Lua file"] {
    get-editbox -after [get-label "Name:"] | set-text "myfile.lua"
    get-button Finish | click
}

// check editor is opened
get-tab-folder -index 2 | get-property activePage | equals myfile | verify-true

// check myfile.lua is in source folder
get-view "Script Explorer" | get-tree | get-item "myproject/src/myfile.lua" | get-property caption 
    | equals "myfile.lua" | verify-true
