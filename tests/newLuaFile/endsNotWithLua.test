// create lua file
get-menu "File/New/Lua File" | click
with [get-window "New Lua file"] {
    get-editbox -after [get-label "Name:"] | set-text myfile
    get-button Finish | click
}

// check editor is opened
get-tab-folder -index 2 | get-property activePage | equals myfile | verify-true

// check myfile.lua is in source folder
wait 200
with [get-view "Script Explorer" | get-tree] { 
	select "myproject/src/myfile.lua" | click
	get-item "myproject/src/myfile.lua" | get-property caption | equals "myfile.lua" | verify-true
}
