// Open the main.lua file and enter a string
with [get-view "Script Explorer" | get-tree] {
    select "myproject/src/main.lua" | double-click
}

with [get-editor main | get-text-viewer] {
    set-caret-pos 2 1
    type-text "a = \"azerty"
    key-type Enter
}

// Check syntaxic coloration for reserved words
//wait 1000
with [get-editor main | get-text-viewer] {
    select-range 2 5 2 13
}
with [get-editor main | get-editbox] {
	// Check if
    get-property "styles.text" -index 12 | equals "azerty" | verify-true
    get-property "styles.fontStyle" -index 12 | equals normal | verify-true
    get-property "styles.foregroundColor" -index 12 | equals "#2A00FF" | verify-true
}
