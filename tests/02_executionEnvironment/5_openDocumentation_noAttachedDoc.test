// Expand the Script Explorer
with [get-view "Script Explorer" | get-tree] {
    select "Project_noExecEnv/src" | double-click
    select "Project_noExecEnv/src/main.lua"
}

// Check documentation is displayed in the "Luadoc" view
wait 200
get-view Luadoc | click
get-view Luadoc | get-control Browser | get-property text | equals "main.lua\n"
	+ "Note: This element has no attached documentation." | verify-true
get-view "Script Explorer" | get-tree | select "Project_noExecEnv/src/main.lua/main"
wait 200
get-view Luadoc | get-control Browser | get-property text | equals "<HTML><HEAD>\\n"
    + "<STYLE type=text/css CHARSET=\"ISO-8859-1\">/* Font definitions */\\n"
