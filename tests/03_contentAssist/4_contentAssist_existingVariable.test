------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

/*********************
*	Created by: Simon Gaestel
*	Creation date: 07/10/2012
*********************/

// Open main.lua file
with [get-view "Script Explorer" | get-tree] {
    select "Project_noExecEnv/src/main.lua" | double-click
}

// Define a new variable, re-use it with the content assist to auto-complete the variable
with [get-editor main | get-text-viewer] {
    set-caret-pos 1 22
    key-type Enter -times 2
    type-text "local variable_custom"
    key-type Enter -times 2
    type-text var
    key-type "M1+SPACE"
    type-text " = 1"
}

// Check the final text
get-editor main | get-editbox | get-property text | equals "local function main()\n\n"
    + "local variable_custom\n\n"
    + "variable_custom = 1\n\n"
    + "end\n"
    + "main()" | verify-true
