Entry-Name: .content

/*********************
*	Created by: Simon Gaestel
*	Creation date: 07/09/2012
*********************/

// Open main.lua file
with [get-view "Script Explorer" | get-tree] {
    select "Project_noExecEnv/src/main.lua" | click -default
    select "Project_noExecEnv/src/main.lua" | double-click
}

// Open the content-assist after a few letters typed
with [get-editor main | get-text-viewer] {
    set-caret-pos 3 4
    key-type Enter -times 2
    type-text loc
    key-type "M1+SPACE"
}

// Check the content assist
get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | get-property itemCount | equals 0
| verify-false

// Use content assist to complete text
with [get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table] {
    select "local - Variable Declaration" | key-type Enter
    select "local - Variable Declaration" | click -default
}

// Check auto-completed text
get-editor main | get-editbox | get-property text | equals "local function main()\n\n"
    + "end\n\n"
    + "local variable\n\n"
    + "main()" | verify-true
