Content-Type: text/ecl
Entry-Name: .content

/*********************
*	Created by: Simon Gaestel
*	Creation date: 07/09/2012
*********************/

// Open main.lua file
with [get-view "Script Explorer" | get-tree] {
    select "Project_noExecEnv/src/main.lua" | click -default
    select "Project_noExecEnv/src/main.lua" | double-click
}

// Open the content-assist after a few letters typed
with [get-editor main | get-text-viewer] {
    set-caret-pos 3 4
    key-type Enter -times 2
    type-text mat
    key-type "M1+SPACE"
}

// Check nothing is available for content assist, and no completion has been done
get-label "No completions available." | get-property caption | equals "No completions available." | verify-true
get-editor main | get-editbox | get-property text | equals "local function main()\n\n"
    + "end\n\n"
    + "mat\n"
    + "main()" | verify-true
