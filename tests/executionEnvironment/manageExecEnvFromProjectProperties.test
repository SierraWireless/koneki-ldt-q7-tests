// access to preference page
get-view "Script Explorer" | get-tree | select myproject | get-menu Properties | click
with [get-window "Properties for myproject"] {
    get-tree | select "Lua/Build Path"
    get-tab-folder | get-tab-item Libraries | click
    get-button "Add Library..." | click
    with [get-window "Add Library"] {
        get-button "Next >" | click
        get-button Configure | click
    }
}

// check the page is opened
get-window "Properties for myproject" | get-window "Add Library" | get-window "Preferences (Filtered)" 
    | get-label "Execution Environments" | get-property caption | equals "Execution Environments" | verify-true
    
// workarround close windows
with [get-window "Properties for myproject"] {
    with [get-window "Add Library"] {
        get-window "Preferences (Filtered)" | get-button Cancel | click
        get-button Cancel | click
    }
    get-button Cancel | click
}
